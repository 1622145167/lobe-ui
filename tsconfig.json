{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "types": ["vitest/globals"],
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@/*": ["src/*"],
      "@lobehub/ui": ["src"],
      "@lobehub/ui/*": ["src/*"]
    }
  },
  "include": ["src", "tests", ".dumi/**/*", ".dumirc.ts", "*.ts"]
}
