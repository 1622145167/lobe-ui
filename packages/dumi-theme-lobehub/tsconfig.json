{
  "include": ["src", "typings", "*.ts"],
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@@/*": ["../../.dumi/tmp/*"],
      "@/*": ["src"],
      "dumi/theme/*": ["../../.dumi/tmp/dumi/theme/*"],
      "@lobehub/ui": ["../../src"],
      "@lobehub/ui/*": ["../../src/*", "*"]
    }
  }
}
