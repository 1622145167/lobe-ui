{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "paths": {
      "@@/*": ["../../.dumi/tmp/*"],
      "@/*": ["src/*"],
      "dumi/theme/*": ["../../.dumi/tmp/dumi/theme/*"],
      "@lobehub/ui": ["../../src"],
      "@lobehub/ui/*": ["../../src/*", "*"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": ["src", "typings", "../../.dumi/**/*", "*.ts"]
}
